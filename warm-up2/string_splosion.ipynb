    "def string_splosion(str):\n",
    " s=\"\"\n",
    " st=\"\"\n",
    " s=[s+str[0:x+1] for x in range(len(str))]\n",
    " for x in s:\n",
    "    st=st+x\n",
    " return st"
   ]
